pipeline {
     options {
        buildDiscarder(
            logRotator(
                artifactDaysToKeepStr: "",
                artifactNumToKeepStr: "",
                daysToKeepStr: "",
                numToKeepStr: "10"
            )
        )
        disableConcurrentBuilds()
    }
  agent {
  dockerfile true
  }
  stages {
    stage('Example') {
       steps {
          echo 'hello world!'
          sh '''
            docker ps
          '''
         }
       }
     }
   }
